{
  "name": "pelican-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:static": "cross-env APP_ENV=static next dev",
    "dev:static:without-image-optimization": "cross-env NODE_ENV=test npm run dev:static",
    "build": "next build",
    "start": "next start",
    "postcss": "postcss src/styles/index.scss --dir build/static/css --parser postcss-scss",
    "start:pre-release": "next start",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:fix": "npm run lint:ts:fix && npm run lint:scss:fix",
    "lint:ts": "next lint",
    "lint:scss": "npx stylelint \"**/*.{css,scss}\"",
    "lint:ts:fix": "next lint --fix",
    "lint:scss:fix": "npm run lint:scss --fix",
    "prepare": "husky",
    "playwright-test": "npx playwright test",
    "playwright-test:ui": "npx playwright test --ui",
    "playwright-e2e-test": "npx playwright test --config=playwright.e2e.config.ts",
    "playwright-e2e-test:ui": "npx playwright test --ui --config=playwright.e2e.config.ts",
    "update-snapshots": "npx playwright test --update-snapshots",
    "generate-api-types": "docker-compose -f ./api-types/docker-compose.yml up --build ts_generator"
  },
  "dependencies": {
    "axe-core": "^4.10.2",
    "axios": "^1.8.4",
    "clsx": "^2.1.0",
    "cookies-next": "^4.1.1",
    "dayjs": "^1.11.13",
    "next": "^14.2.28",
    "plyr-react": "^5.3.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "qs": "^6.14.0",
    "react": "^18",
    "react-dom": "^18",
    "react-focus-lock": "^2.13.2",
    "react-glider": "^4.0.2",
    "react-markdown": "^9.0.1",
    "rehype-raw": "^7.0.0",
    "sharp": "^0.34.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@playwright/test": "^1.52.0",
    "@stylistic/eslint-plugin": "^1.6.3",
    "@stylistic/stylelint-config": "^1.0.1",
    "@types/node": "^20",
    "@types/qs": "^6.9.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-plyr": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "autoprefixer": "^10.4.19",
    "axe-html-reporter": "^2.2.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "husky": "^9.0.11",
    "postcss-cli": "^11.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "sass": "^1.87.0",
    "stylelint": "^16.2.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5"
  }
}
